<Window x:Class="stockExample.DashBoardWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:stockExample"
        mc:Ignorable="d"
        Title="DashBoardWindow" Height="450" Width="800">
    <Grid>
        <StackPanel Orientation="Horizontal">
            <Button Width="100px" Margin="30,170,30,161" Content="통합주문" Click="enterIntegrationOrderWindow"/>
            <Button Width="100px" Margin="30,170,30,161" Content="입출금" Click="enterDepositAndWithdrawalWindow"/>
            <Button Width="100px" Margin="30,170,30,161" Content="당일거래내역" Click="enterTransactionHistoryWindow"/>
            <Button Width="100px" Margin="30,170,30,161" Content="일별손익" Click="enterProfitAndLossWindow"/>
        </StackPanel>


    </Grid>
</Window>
